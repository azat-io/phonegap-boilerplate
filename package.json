{
  "name": "phonegap-boilerplate",
  "description": "PhoneGap boilerplate",
  "author": "Azat S. <to@azat.io> (http://azat.io)",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server",
    "build": "npm-run-all --parallel build:*",
    "build:android": "phonegap build android",
    "build:ios": "phonegap build ios",
    "build:winphone": "phonegap build winphone",
    "lint": "npm-run-all --parallel lint:*",
    "lint:pug": "./node_modules/.bin/pug-lint \"./src/pug/index.pug\"",
    "lint:css": "stylelint \"./src/postcss/**/*.css\"",
    "lint:js": "./node_modules/.bin/eslint --fix ./src/es6/*.es6",
    "splashicon": "splashicon-generator --imagespath=\"project/assets\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/azat-io/phonegap-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/azat-io/phonegap-boilerplate/issues"
  },
  "homepage": "https://github.com/azat-io/phonegap-boilerplate",
  "devDependencies": {
    "babel-core": "6.11.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "6.9.0",
    "css-loader": "0.23.1",
    "css-mqpacker": "5.0.1",
    "eslint": "3.2.0",
    "eslint-config-standard": "5.3.5",
    "eslint-plugin-promise": "2.0.0",
    "eslint-plugin-standard": "2.0.0",
    "normalize.css": "4.2.0",
    "npm-run-all": "2.3.0",
    "postcss": "5.1.1",
    "postcss-browser-reporter": "0.5.0",
    "postcss-cssnext": "2.7.0",
    "postcss-import": "8.1.2",
    "postcss-inline-svg": "2.0.1",
    "postcss-loader": "0.9.1",
    "postcss-modules": "0.5.0",
    "postcss-reporter": "1.4.1",
    "postcss-responsive-type": "0.4.0",
    "postcss-short": "1.4.0",
    "postcss-uncss": "0.14.0",
    "pre-commit": "1.1.3",
    "pug": "2.0.0-beta4",
    "pug-lint": "2.3.0",
    "pug-lint-config-clock": "2.0.0",
    "pug-loader": "2.1.0",
    "splashicon-generator": "^0.2.9",
    "style-loader": "0.13.1",
    "stylelint": "7.0.3",
    "stylelint-config-standard": "11.0.0",
    "uncss": "^0.14.1",
    "webpack": "1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-validator": "2.2.6"
  }
}
